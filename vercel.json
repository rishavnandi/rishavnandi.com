{
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/tw",
      "destination": "https://twitter.com/rishav__nandi"
    },
    {
      "source": "/x",
      "destination": "https://twitter.com/rishav__nandi"
    },
    {
      "source": "/ln",
      "destination": "https://www.linkedin.com/in/rishavnandi/"
    },
    {
      "source": "/gh",
      "destination": "https://github.com/rishavnandi"
    },
    {
      "source": "/git",
      "destination": "https://github.com/rishavnandi/rishavnandi.com"
    }
  ]
}